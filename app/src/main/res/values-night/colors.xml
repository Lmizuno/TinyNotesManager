<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Dark theme colors -->
    <color name="primary">#011015</color>
    <color name="primary_variant">#021E2D</color>
    <color name="color_on_primary">#FFFFFFFF</color>

    <!-- SECONDARY COLOR SETTINGS -->
    <color name="secondary">#180106</color>
    <color name="secondary_variant">#1A0103</color>
    <color name="color_on_secondary">#FFFFFFFF</color>

    <!-- FADED -->
    <color name="primary_faded">#042733</color>
    <color name="primary_variant_faded">#0A4C71</color>
    <color name="secondary_faded">#B35066</color>
    <color name="secondary_variant_faded">#793237</color>

    <color name="chip_background">@color/primary_variant_faded</color>
    <color name="chip_text">@color/white</color>
    <color name="chip_selected_background">@color/secondary_variant_faded</color>
    <color name="chip_selected_text">@color/white</color>
    <color name="chip_ripple">@color/gray</color>

    <color name="mtrl_textinput_default_box_stroke_color" tools:override="true" tools:ignore="MissingDefaultResource">#042733</color>
</resources>